    "@subwallet-connect/injected-wallets": "^1.0.8",
    "@subwallet-connect/subwallet": "^1.0.8",
    "@subwallet-connect/subwallet-polkadot": "^1.0.8",
    "@tresjs/cientos": "^4.0.3",
    "@tresjs/core": "^4.3.1",
    "bignumber.js": "^9.1.2",
    "ethers": "^6.13.4",
    "gsap": "^3.12.5",
